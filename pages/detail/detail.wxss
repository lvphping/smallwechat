.warp {
    height: 100%;
    display: flex;
    flex-direction: column;
    padding: 20rpx;
    font-size: 16px;
}
.title {
    text-align: center;
    padding: 20rpx;
    font-size: 20px;
    font-weight:bold;
}
.cTime {
    color: #aaa;
    font-size:14px;
    margin-bottom:8px;
}
.img {
    text-align: center;
    padding: 20rpx;
}
.img image {
    width: 120px;
    height: 120px;
}
.content {
    text-indent: 2em;
    font-size:15px;
    font-family:'微软雅黑';
    color:#666;
}
.close {
    text-align: center;
    margin: 30px;
    font-size: 20px;
    color: #aaa
}
.info{
    text-align:center;
}